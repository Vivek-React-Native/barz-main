{
  "name": "@barz/mobile",
  "version": "1.0.2",
  "main": "./src/index.js",
  "scripts": {
    "start": "expo start --dev-client",
    "android": "expo run:android",
    "open:android": "open -a \"Android Studio\" android",
    "android:reverse": "adb reverse tcp:8000 tcp:8000 && adb reverse tcp:8081 tcp:8081",
    "ios": "expo run:ios",
    "open:ios": "open -a \"Xcode\" ios",
    "web": "expo start --web",
    "check": "NODE_OPTIONS=--max-old-space-size=8192 tsc --watch --noEmit",
    "prettier-check": "prettier --check '{src,e2e}/**/*.{ts,tsx,scss}'",
    "prettier-format": "prettier --write '{src,e2e}/**/*.{ts,tsx,scss}'",
    "format": "npm run prettier-format",
    "detox:build": "time npx detox build -c ios.sim.debug",
    "detox:test": "time npx detox test --configuration ios.sim.debug",
    "test": "npm run detox:build && npm run detox:test",
    "fetch-icons": "./fetch-icons.sh",
    "eas:build:ios": "eas build --platform ios --profile preview",
    "eas:build:android": "eas build --platform android --profile preview",
    "eas:build": "eas build --profile preview",
    "eas:build:ios:production": "eas build --platform ios --profile production",
    "eas:update:ios": "eas update --platform ios --channel preview",
    "eas:update:android": "eas update --platform android --channel preview",
    "eas:update:ios:production": "eas update --platform ios --channel production",
    "eas:update:android:production": "eas update --platform android --channel production",
    "eas-build-pre-install": "./eas-scripts/eas-build-pre-install.sh",
    "eas-build-post-install": "./eas-scripts/eas-build-post-install.sh",
    "postinstall": "mkdir -p node_modules/@barz && rm -rf node_modules/@barz/mobile && ln -s ../../ node_modules/@barz/mobile"
  },
  "dependencies": {
    "@babel/runtime": "^7.23.1",
    "@barz/twilio-video": "*",
    "@clerk/clerk-expo": "^0.17.4",
    "@expo/react-native-action-sheet": "^4.0.1",
    "@gorhom/bottom-sheet": "^4.4.7",
    "@pusher/pusher-websocket-react-native": "^1.2.1",
    "@react-native-async-storage/async-storage": "1.17.11",
    "@react-native-community/netinfo": "^9.3.7",
    "@react-navigation/bottom-tabs": "^6.5.7",
    "@react-navigation/native": "^6.1.6",
    "@react-navigation/native-stack": "^6.9.12",
    "@sentry/react-native": "^4.15.2",
    "color-alpha": "^1.1.3",
    "date-fns": "^2.30.0",
    "expo": "~48.0.21",
    "expo-application": "~5.1.1",
    "expo-auth-session": "^4.0.3",
    "expo-build-properties": "~0.6.0",
    "expo-camera": "^13.2.1",
    "expo-clipboard": "~4.1.2",
    "expo-constants": "~14.2.1",
    "expo-crypto": "^12.2.2",
    "expo-device": "~5.2.1",
    "expo-file-system": "~15.2.2",
    "expo-font": "^11.1.1",
    "expo-haptics": "^12.2.1",
    "expo-image-picker": "^14.1.1",
    "expo-keep-awake": "^12.0.1",
    "expo-linear-gradient": "^12.1.2",
    "expo-linking": "^6.0.0",
    "expo-secure-store": "^12.1.1",
    "expo-sharing": "^11.2.2",
    "expo-splash-screen": "~0.18.2",
    "expo-status-bar": "~1.4.4",
    "expo-updates": "^0.17.0",
    "expo-web-browser": "^12.1.1",
    "lodash.debounce": "^4.0.8",
    "phone": "^3.1.37",
    "querystring": "^0.2.1",
    "react": "18.2.0",
    "react-native": "0.71.14",
    "react-native-exit-app": "^1.1.0",
    "react-native-flash-message": "^0.4.2",
    "react-native-gesture-handler": "~2.9.0",
    "react-native-keyboard-aware-scroll-view": "^0.9.5",
    "react-native-navigation-bar-color": "^2.0.2",
    "react-native-reanimated": "2.14.4",
    "react-native-restart": "^0.0.27",
    "react-native-safe-area-context": "4.5.0",
    "react-native-screens": "~3.20.0",
    "react-native-share": "^9.2.3",
    "react-native-svg": "^13.4.0",
    "react-native-version-number": "^0.3.6",
    "react-native-video": "^5.2.1",
    "semver": "^7.5.1",
    "sentry-expo": "~6.2.0",
    "xstate": "^4.37.2"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@clerk/clerk-sdk-node": "^4.10.2",
    "@config-plugins/detox": "^5.0.1",
    "@types/lodash.debounce": "^4.0.7",
    "@types/react": "~18.0.38",
    "@types/react-native-video": "^5.0.14",
    "@types/uuid": "^9.0.1",
    "detox": "^20.9.0",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "http-terminator": "^3.2.0",
    "jest": "^29.5.0",
    "puppeteer": "^20.4.0",
    "socket.io": "^4.6.1",
    "socket.io-client": "^4.6.1",
    "typescript": "^4.9.4"
  },
  "private": true,
  "expo": {
    "autolinking": {
      "nativeModulesDir": "../../packages/twilio-video/"
    }
  }
}
